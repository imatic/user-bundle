        "imatic/view-bundle": "~3.0",
        "imatic/controller-bundle": "~3.0"
    },
    "require-dev": {
        "imatic/testing-bundle": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Imatic\\Bundle\\UserBundle\\": ""
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "suggest": {
        "phpoffice/phpexcel": "To create and import role documents."
    }
