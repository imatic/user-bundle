{
    "name": "imatic/user-bundle",
    "type": "symfony-bundle",
    "description": "Imatic User Bundle",
    "keywords": [],
    "homepage": "http://imatic.cz",
    "license": "proprietary",
    "authors": [
        {
            "name": "Stepan Koci",
            "email": "stepan.koci@imatic.cz"
        },
        {
            "name":"Imatic Software s.r.o.",
            "homepage":"http://imatic.cz/"
        }
    ],
    "require": {
        "php":">=5.4",
        "symfony/framework-bundle":"~2.4",
        "friendsofsymfony/user-bundle": "2.0.*",
        "doctrine/orm": ">=2.2.3,<2.5-dev",
        "doctrine/doctrine-bundle": ">=1.2",
        "symfony/swiftmailer-bundle": "~2.3",
        "imatic/view-bundle": "*",
        "imatic/controller-bundle": "*",
        "imatic/importexport-bundle": "*",
        "symfony/assetic-bundle": "~2.3"
    },
    "require-dev": {
        "imatic/testing-bundle": "dev-master"
    },
    "autoload": {
        "psr-0": { "Imatic\\Bundle\\UserBundle": "" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "target-dir": "Imatic/Bundle/UserBundle",
    "extra": {
        "branch-alias":{
            "dev-master":"2.4.x-dev"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php -r \"if (!file_exists('Tests/Fixtures/TestProject/bower.json')) symlink(__DIR__ . '/vendor/imatic/view-bundle/Imatic/Bundle/ViewBundle/Tests/Fixtures/TestProject/bower.json', 'Tests/Fixtures/TestProject/bower.json');\"",
            "php -r \"if (!file_exists('Tests/Fixtures/TestProject/.bowerrc')) symlink(__DIR__ . '/vendor/imatic/view-bundle/Imatic/Bundle/ViewBundle/Tests/Fixtures/TestProject/.bowerrc', 'Tests/Fixtures/TestProject/.bowerrc');\""
        ]
    },
    "suggest": {
        "phpoffice/phpexcel": "To create and import role documents."
    }
}
